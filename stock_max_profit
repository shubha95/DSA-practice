var maxProfit = function(prices) {
    let sell=1
    let buy=0;
    let maxProfit=0;
    while(sell<prices.length){
        if(prices[sell]>prices[buy]){
            let profit=prices[sell]-prices[buy]
            maxProfit=Math.max(maxProfit,profit)
        }else{
            buy=sell
        }
        sell++
    }
    return maxProfit
    
};
