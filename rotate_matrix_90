/**
 * @param {number[][]} matrix
 * @return {void} Do not return anything, modify matrix in-place instead.
 */
var rotate = function(matrix) {
    let n = matrix.length;
    for(let layer=0; layer<n/2; layer++){
        let first = layer;
        let last = n - 1 - layer

       for(let i = first ;i< last ; i++){
          let innerindex = i - first
          let top = matrix[first][i]
           matrix[first][i] = matrix[last - innerindex][first]
           matrix[last - innerindex][first] = matrix[last][last - innerindex]
           matrix[last][last - innerindex] = matrix[i][last]
           matrix[i][last] = top

       }

    }
    return matrix
};
